
public class DistinctSebsequences {
	public int numDistinct(String S, String T) {
        int m = S.length();
        int n = T.length();
        
        if(m==0 || n==0) 
            return 0;
        
        char s[] = S.toCharArray();
        char t[] = T.toCharArray();
        
        int dp[][] = new int[n+1][m+1];
        for(int i = 0; i<=n; i++)
            dp[i][0] = 0;
        for(int j = 0; j<=m; j++)
            dp[0][j] = 1;
        
        
        for(int i = 1; i<=n; i++)
            for(int j = 1; j<=m; j++)
            {
                if(s[j-1]==t[i-1])
                    dp[i][j] = dp[i-1][j-1] + dp[i][j-1];
                else
                    dp[i][j] = dp[i][j-1];
            }
        return dp[n][m];    
    }


    public static void main(String args[]){
        String S = new String("ccc");
        String T = new String("c");
        DistinctSebsequences s = new DistinctSebsequences();
        System.out.println(s.numDistinct(S, T));
    }

}
